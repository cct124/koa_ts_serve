{
  "name": "koa_ts_serve",
  "version": "1.0.0",
  "private": true,
  "description": "基于koa和TypeScript的node服务",
  "scripts": {
    "serve": "cross-env NODE_ENV=development node dist/main.js --inspect",
    "watch:node": "nodemon --watch dist --exec npm run serve -L",
    "watch:ts": "tsc -w -p tsconfig.json",
    "dev": "concurrently -k -p \"{name}:\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run watch:node\"",
    "build": "rm -rf dist/* && tsc",
    "start": "cross-env NODE_ENV=production node dist/main.js"
  },
  "keywords": [
    "node",
    "serve",
    "TS",
    "TypeScript",
    "Koa",
    "Swagger"
  ],
  "author": "Jane",
  "license": "MIT",
  "devDependencies": {
    "@types/formidable": "^1.0.31",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa2-cors": "^2.0.1",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "formidable": "^1.2.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "koa-swagger-decorator": "^1.7.3",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.3.0"
  }
}
